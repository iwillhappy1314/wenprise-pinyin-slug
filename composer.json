{
  "name": "wenprise/pinyin-slug",
  "description": "Convert Chinese post, taxonomy tag, uploaded filename to pinyin in WordPress.",
  "require": {
    "php": ">=7.4",
    "overtrue/pinyin": "^4.0",
    "tareq1988/wordpress-settings-api-class": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "WenprisePinyinSlug\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Amos Lee",
      "email": "iwillhappy1314@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "require-dev": {
    "yoast/phpunit-polyfills": "^1.0",
    "phpunit/phpunit": "^7.5 || ^8.5",
    "wp-cli/wp-cli-bundle": "*"
  }
}
